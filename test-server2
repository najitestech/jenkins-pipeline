node {
   stage('One') {
       sh 'uname -a'
       git 'https://github.com/najitestech/jenkins-pipeline.git'
       echo 'Stage one was successful'
   }
   }
   
node {
   stage('Two') {
      sshagent(['jenkins-server-pkey']) {
         sh 'uname -a'
         sh 'ls -la'
         sh 'cat /etc/*-release'
   }
   }
   }
   
node {
   stage('Three') {
      sshagent(['jenkins-server-pkey']) {
         sh 'uptime'
         sh 'ip a'
         sh 'uname -a'
         //sh 'scp -o StrictHostKeyChecking=no /root/public root@192.168.0.26:/home'
         echo 'Stage three was successful'
   }
   }
   }
