pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...This is just a testing pipeline'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('Monitoring') {
            steps {
                echo 'Monitoring..'
            }
        }
        stage(Maturity) {
            node {
                  def job = build job: 'Deploying-to-vCenter'
            }
        }
    }
}

