node ('Ansible-master') {
   stage('One') {
       git 'https://github.com/najitestech/jenkins-pipeline.git'
   }
 }
  
node ('Ansible-master') {
   stage('Two.*') {
       echo 'ansible-playbook new-vm.yml'
   }
 }
 
pipeline {
agent any
    stages {
        stage("build") {
            steps {
                build('jnks-slave-job2')
                build('jnks-slave-job3')
            }
        }
    }
}
