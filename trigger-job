node ('jnks_slave_job3') {
   stage('One') {
       git 'https://github.com/najitestech/jenkins-pipeline.git'
       echo 'Stage one was successful'
   }
 }
  
node ('jnks_slave_job3') {
   stage('Two') {
       echo 'Stage two was successful'
   }
 }
 
 node ('jnks_slave_job3') {
    stage("Three") {
        parallel (
            "Third Build" : {
                build("check-date")
                echo 'Stage three was successful'
            },
        )
    }
}

 node ('jnks_slave_job3') {
    stage("Final") {
        parallel (
            "Final Build" : {
                build("jenkins-pippeline-job/master")
                build("jenkins-pippeline-job/future-branch")
                build("Deploying-to-vCenter")
            },
        )
    }
}
